	"tokenColors": [
		{ //comment
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"foreground": "{{com}}"
			}
		},
		{ //Identifier
			"name": "Normal",
			"scope": [
				"variable"
				// "entity",
			],
			"settings": {
				"foreground": "{{fg}}"
			}
		},
		{
			//Statement,Lable,Keyword,Exception,
			//Storage,Type(class),Structure,Typedef,Tag,
			//Special,SpecialChar,SpecialComment,
			//operator,language,property
			"name": "primary",
			"scope": [
				//statement
				"statement",
				"lable",
				"keyword",
				"keyword.control.exception",
				"keyword.control.try",
				"keyword.control.catch",
				"keyword.control.throw",
				//storage
				"storage",
				"storage.modifier",
				"storage.type",
				"meta.type",
				"meta.return-type",
				"support.type",
				"entity.name.type",
				"meta.class",
				"support.class",
				"entity.name.class",
				"entity.other.inherited-class",
				"storage.type.struct",
				"keyword.other.typedef",
				"meta.tag",
				"entity.name.tag",
				"entity.name.section",
				//special
				"constant.character.escape",
				"string.regexp",
				"variable.language",
				//operator
				"operator",
				"entity.name.function.operator",
				"keyword.operator",
				"support",
				//Property
				"property",
				"variable.object.property",
				"variable.other.property",
				"variable.other.object.property",
				"variable.other.constant.property"
			],
			"settings": {
				"foreground": "{{prim}}"
			}
		},
		{ //Constant,String,Character,Number,Boolean,Float
			"name": "secondary",
			"scope": [
				"constant",
				"constant.character",
				"constant.numeric",
				"constant.numeric.float",
				"string",
				"support.constant"
			],
			"settings": {
				"foreground": "{{seco}}"
			}
		},
		{ //PreProc,Include,Define,Macro,Function
			"name": "tertiary",
			"scope": [
				"entity.name.namespace",
				"keyword.control.directive",
				"keyword.control.directive.define",
				"entity.name.function.preprocessor",
				"meta.preprocessor.macro",
				"meta.function",
				"meta.function-call",
				"entity.name.function",
				"support.function",
				"punctuation.definition.directive"
			],
			"settings": {
				"foreground": "{{tert}}"
			}
		},
		{ //Coditional,Repeat
			"name": "quaternary",
			"scope": [
				"keyword.control.conditional",
				"keyword.control.repeat",
				"keyword.control.flow",
				"keyword.control.if",
				"keyword.control.fi",
				"keyword.control.then",
				"keyword.control.else",
				"keyword.control.elif",
				"keyword.control.switch",
				"keyword.control.case",
				"keyword.control.esac",
				"keyword.control.do",
				"keyword.control.while",
				"keyword.control.in",
				"keyword.control.for",
				"keyword.control.break",
				"keyword.control.continue"
				// "keyword.control.return",
			],
			"settings": {
				"foreground": "{{quat}}"
			}
		},
		{ //invalid
			"name": "err",
			"scope": [
				"invalid",
				"markup.deleted"
			],
			"settings": {
				"foreground": "{{err}}"
			}
		},
		{ //Delimiter
			"name": "quinary",
			"scope": [
				// "punctuation",
				"meta.tail.struct",
				"punctuation.separator",
				"punctuation.section",
				"punctuation.definition.parameters",
				"punctuation.definition.array",
				"punctuation.definition.dictionary",
				"punctuation.terminator.statement"
			],
			"settings": {
				"foreground": "{{quin}}"
			}
		},
		{ //paramrter
			"name": "fg-s",
			"scope": [
				"parameter",
				"variable.parameter"
			],
			"settings": {
				"foreground": "{{fgs}}"
			}
		},
		{
			"name": "Italics",
			"scope": [
				"meta.parameters",
				"meta.type.parameters",
				"meta.return.type",
				"entity.name.type.interface",
				"meta.type.annotation",
				"meta.function.parameters",
				"markup.italic.markdown"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold.markdown",
				"storage.type.annotation.dart"
			],
			"settings": {
				"fontStyle": "bold"
			}
		}
	]
}